<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link  rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}">
            <link  rel="stylesheet" href="{{ asset('css/style.css') }}">
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
            {# <a class="navbar-brand" href="#"></a> #}
            <button class="navbar-toggler d-lg-none" type="button" data-toggle="collapse" data-target="#collapsibleNavId" aria-controls="collapsibleNavId"
                aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="collapsibleNavId">
                <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                    <li class="nav-item active">
                        <a class="nav-link" href="{{ path('type_index') }}">Type</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('sous_type_index') }}">Sous-type</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('departement_index') }}">Département</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('materiel_index') }}">Matériel</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('materiel_entree_index') }}">Matériel entrée</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('materiel_sortie_index') }}">Matériel sortie</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('client_index') }}">Client</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('facture_index') }}">Facture</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('facturation') }}">Facturation</a>
                    </li>
                </ul>
            </div>
        </nav>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
